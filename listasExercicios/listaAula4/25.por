		area=lado*lado
		pisos=0.3*area
		escreva("\nA área do chão da cozinha é de "+area+" metros quadrados.")
		escreva("\nVão ser utilizados "+pisos+" pisos para cobrir o chão.")
		
		
		
		
		
		
		
		
		
	}
}